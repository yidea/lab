#!/usr/bin/env node

var sys = require("sys"),
  exec = require("child_process").exec,
  util = require("util");

function puts(error, stdout, stderr) { 
//  sys.puts(stdout);
  console.log(process.env.PORT);
  console.log(error);
}

var cmd = util.format("curl http://localhost:%s/weather", process.env.PORT || 3000);
exec(cmd, puts);
