{{#section "css"}}
  <link rel="stylesheet" href="/bower_components/todomvc-common/base.css">
  <link rel="stylesheet" href="/styles/app/todo.css">
{{/section}}

<div class="js-todo todo" id="todoapp">
  <header id="header">
    <h1>Todo</h1>
    <input id="new-todo" placeholder="Enter here.." autofocus>
  </header>
  <section id="main">
    <input id="toggle-all" type="checkbox">
    <label for="toggle-all">Mark</label>
    <ul id="todo-list"></ul>
  </section>
  <footer id="footer"></footer>
</div>
